.iui-switch {
  display: inline-block;
  vertical-align: middle;
}

.iui-switch input {
  position: relative;
  width: 52px;
  height: 31px;
  -webkit-appearance: none;
  border: none;
  background: none;
  display: block;
}

.iui-switch input:before {
  content: '';
  display: block;
  width: 100%;
  height: 100%;
  border: 1px solid #dfe0e1; /*no*/
  background-color: #fff;
  border-radius: 15.5px;
  box-shadow: #dfdfdf 0 0 0 0 inset;
  background-clip: content-box;
}

.iui-switch input:checked:before {
  border-color: #64bd63;
  background-color: #64bd63;
  -webkit-transition: all ease 400ms;
  transition: all ease 400ms;
  box-shadow: #64bd63 0 0 0 16px inset;
}

.iui-switch input:after {
  content: '';
  width: 29px;
  height: 29px;
  position: absolute;
  top: 1px;
  left: 0;
  border-radius: 50%;
  background-color: #fff;
  box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.4);
  -webkit-transition: 300ms;
  transition: 300ms;
  background-clip: padding-box;
}

.iui-switch input:checked:after {
  left: 21px;
}

.iui-radio {
  display: inline-block;
}

.iui-radio input {
  -webkit-appearance: none;
  position: relative;
  display: inline-block;
  margin-right: 5px;
  width: 24px;
  height: 24px;
  vertical-align: middle;
  border: 0;
  background: none;
}

.iui-radio input:before {
  content: '';
  display: block;
  width: 100%;
  height: 100%;
  border: 1px solid #dfe0e1; /*no*/
  border-radius: 50%;
  background-color: #fff;
}

.iui-radio input:checked:before {
  border-color: $c-primary;
}

.iui-radio input:checked:after {
  content: '';
  position: absolute;
  left: 25%;
  top: 25%;
  width: 50%;
  height: 50%;
  background: $c-primary;
  border-radius: 50%;
}

.iui-checkbox {
  display: inline-block;
}

.iui-checkbox input[type=checkbox] {
  -webkit-appearance: none;
  position: relative;
  width: 24px;
  height: 24px;
  margin-right: 5px;
  vertical-align: middle;
  background: none;
  border: none;
}

.iui-checkbox input[type=checkbox]:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  border: 1px solid #dfe0e1; /*no*/
  background-color: #fff;
}

.iui-checkbox input[type=checkbox]:checked:before {
  background: $c-primary;
  border-color: $c-primary;
}

.iui-checkbox input[type=checkbox]:checked:after {
  content: '';
  position: absolute;
  width: 48%;
  height: 25%;
  top: 32%;
  left: 27%;
  border: 2px solid #fff;
  border-top: none;
  border-right: none;
  -webkit-transform: rotate(-45deg);
  transform: rotate(-45deg);
}

.iui-checkbox-square input[type=checkbox]:before {
